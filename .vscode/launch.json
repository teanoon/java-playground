{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch Graphql Application",
            "request": "launch",
            "mainClass": "com.example.graphql.Application",
            "projectName": "graphql"
        },
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Launch Consistent Hash Application",
            "request": "launch",
            "mainClass": "com.example.hash.Application",
            "projectName": "consistent-hash"
        },
        {
            "type": "java",
            "name": "Launch HotSwap Application",
            "request": "launch",
            "mainClass": "com.example.hotswap.Application",
            "vmArgs": "-XX:HotswapAgent=core",
            "projectName": "hot-swap"
        }
    ]
}
